    <script>


        $(document).ready(function () {
            $('#categorylist').DataTable({
                "ajax": {
                    "url": "/Admin/CategoryList",
                    "type": "GET",
                    "datatype": "json"
                },
                fields: [ {
                    label: 'CreatedTime:',
                    name: 'first_name',
                    def: function () { return new Date(); }
                }],
                "columnDefs": [

            {
                "targets": [ 0 ],
                "visible": false
            }
                ],
                "columns": [
                      { "data": "id" },
                     { "data": "CategoryName" },
                      { "data": "CategoryDiscription" },
                      { "data": "CreatedTime", type: 'date-dd-mmm-yyyy', targets: 0 },
                       {

                             "render": function (data, type, row, meta)
                             { return '<button type="button" class="btn btn-success">Approved</button>'; }
        },
                         {

                             "render": function (data, type, row, meta)
                             { return '<a href="/Admin/EditRole/' + row.id + '"><i class="fa fa-pencil-square-o"></i></a>'; }
                         },
                       {
                           data: null, render: function (data, type, row) {
                               return '<a href="/Admin/DeleteRole/' + row.id + '"><i class="fa fa-trash-o fa-lg"></i></a>';
                           }
                       },
                ]
            });
        });

